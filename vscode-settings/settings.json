{
	"editor.fontSize": 16,
	"security.workspace.trust.untrustedFiles": "open",
	"liveServer.settings.donotShowInfoMsg": true,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.acceptSuggestionOnCommitCharacter": false,
	"prettier.tabWidth": 4,
	"workbench.iconTheme": "material-icon-theme",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"source.organizeImports": "explicit",
	},
	"liveServer.settings.ChromeDebuggingAttachment": false,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.printWidth": 120,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.accessibilitySupport": "off",
	"editor.fontLigatures": false,
	"git.openRepositoryInParentFolders": "always",
	"editor.acceptSuggestionOnEnter": "off",
	"github.copilot.editor.enableAutoCompletions": true,
	"workbench.tree.indent": 16,
	"github.copilot.advanced": {},
	"editor.fontFamily": "ComicShannsMono Nerd Font Mono, 'MesloLGS NF', Menlo, Monaco, 'Courier New', monospace",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.detectIndentation": false,
	"editor.occurrencesHighlight": "off",

	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.useTabs": true,
	"editor.insertSpaces": false,
	"editor.scrollBeyondLastLine": false,
	"files.exclude": {
		".*": true,
		"*-lock.*": true,
		"**/.DS_Store": false,
		"**/.git": false,
		"**/.hg": false,
		"**/.svn": false,
		"**/CVS": false,
		"**/Thumbs.db": false,
		"node_modules": true
	},
	"typescript.inlayHints.variableTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"files.associations": {
		"*.svg": "html"
	},
	"window.commandCenter": false,
	"workbench.colorTheme": "Atom One Dark",
	"go.toolsManagement.autoUpdate": true,
	// neovim settings
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvimvscode/init.lua",
	"vscode-neovim.compositeKeys": {
		"jk": {
			"command": "vscode-neovim.escape"
		},
		"kj": {
			"command": "vscode-neovim.escape"
		},
		"JK": {
			"command": "vscode-neovim.escape"
		},
		"KJ": {
			"command": "vscode-neovim.escape"
		},
	},
	"vscode-neovim.compositeTimeout": 150,
	"vscode-neovim.autoGeneratedDocumentSchemes": [
		"output",
		"vscode-chat-code-block"
	],
	"vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
	"workbench.list.smoothScrolling": true,
	"editor.smoothScrolling": true,
	"vscode_custom_css.imports": ["file:///Users/mac/Desktop/repos/dotfiles/nvimvscode/index.js"],
	"workbench.colorCustomizations": {
		"editorCursor.background": "#000000"
	},
	"editor.cursorSmoothCaretAnimation": "on",
	"git.confirmSync": false,
	"editor.foldingImportsByDefault": true,
	"editor.formatOnSave": true,
}
